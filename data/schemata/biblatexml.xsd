<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://biblatex-biber.sourceforge.net/biblatexml" xmlns:biblatexml="http://biblatex-biber.sourceforge.net/biblatexml">
  <xs:element name="entries">
    <xs:complexType>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="biblatexml:BibLaTeXML.entries"/>
    </xs:complexType>
  </xs:element>
  <xs:group name="BibLaTeXML.entries">
    <xs:choice>
      <xs:group ref="biblatexml:BibLaTeXML.standard.entries"/>
      <xs:group ref="biblatexml:BibLaTeXML.additional.entries"/>
    </xs:choice>
  </xs:group>
  <xs:group name="BibLaTeXML.standard.entries">
    <xs:choice>
      <xs:group ref="biblatexml:article"/>
      <xs:group ref="biblatexml:book"/>
      <xs:group ref="biblatexml:booklet"/>
      <xs:group ref="biblatexml:collection"/>
      <xs:group ref="biblatexml:inbook"/>
      <xs:group ref="biblatexml:incollection"/>
      <xs:group ref="biblatexml:proceedings"/>
      <xs:group ref="biblatexml:inproceedings"/>
      <xs:group ref="biblatexml:manual"/>
      <xs:group ref="biblatexml:misc"/>
      <xs:group ref="biblatexml:online"/>
      <xs:group ref="biblatexml:patent"/>
      <xs:group ref="biblatexml:periodical"/>
      <xs:group ref="biblatexml:proceedings"/>
      <xs:group ref="biblatexml:report"/>
      <xs:group ref="biblatexml:thesis"/>
      <xs:group ref="biblatexml:unpublished"/>
      <xs:group ref="biblatexml:set"/>
    </xs:choice>
  </xs:group>
  <xs:group name="BibLaTeXML.additional.entries">
    <xs:choice>
      <xs:group ref="biblatexml:review"/>
      <xs:group ref="biblatexml:review.electronic"/>
      <xs:group ref="biblatexml:article.electronic"/>
      <xs:group ref="biblatexml:periodical.electronic"/>
      <xs:group ref="biblatexml:preprint"/>
      <xs:group ref="biblatexml:software"/>
      <xs:group ref="biblatexml:movie"/>
    </xs:choice>
  </xs:group>
  <xs:group name="BibLaTeXML.common.fields">
    <xs:annotation>
      <xs:documentation/>
    </xs:annotation>
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="biblatexml:nameaddon"/>
        <xs:element ref="biblatexml:shortauthor"/>
        <xs:element ref="biblatexml:shorthand"/>
        <xs:element ref="biblatexml:shorthandintro"/>
        <xs:element ref="biblatexml:shorttitle"/>
        <xs:element ref="biblatexml:origtitle"/>
        <xs:element ref="biblatexml:origyear"/>
        <xs:element ref="biblatexml:language"/>
        <xs:element ref="biblatexml:origlanguage"/>
        <xs:element ref="biblatexml:abstract"/>
        <xs:element ref="biblatexml:annotation"/>
        <xs:choice>
          <xs:element ref="biblatexml:file"/>
          <xs:element ref="biblatexml:pdf"/>
        </xs:choice>
        <xs:element ref="biblatexml:private"/>
        <xs:element ref="biblatexml:entrysubtype"/>
        <xs:element ref="biblatexml:execute"/>
        <xs:element ref="biblatexml:hyphenation"/>
        <xs:element ref="biblatexml:indexsorttitle"/>
        <xs:element ref="biblatexml:indextitle"/>
        <xs:element ref="biblatexml:keywords"/>
        <xs:element ref="biblatexml:label"/>
        <xs:element ref="biblatexml:options"/>
        <xs:element ref="biblatexml:presort"/>
        <xs:choice>
          <xs:element ref="biblatexml:sortkey"/>
          <xs:element ref="biblatexml:key"/>
        </xs:choice>
        <xs:element ref="biblatexml:sortname"/>
        <xs:element ref="biblatexml:sorttitle"/>
        <xs:element ref="biblatexml:sortyear"/>
        <xs:group ref="biblatexml:inset"/>
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:group name="BibLaTeXML.additional.fields">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="biblatexml:reprintinfo"/>
        <xs:element ref="biblatexml:reviews"/>
        <xs:element ref="biblatexml:topics"/>
        <xs:element ref="biblatexml:remarks"/>
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:group name="formattext">
    <xs:sequence>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element minOccurs="0" ref="biblatexml:emphasis"/>
        <xs:element minOccurs="0" ref="biblatexml:superscript"/>
        <xs:element minOccurs="0" ref="biblatexml:subscript"/>
      </xs:sequence>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="formattextB" mixed="true">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element minOccurs="0" ref="biblatexml:nosort"/>
      <xs:element minOccurs="0" ref="biblatexml:emphasis"/>
      <xs:element minOccurs="0" ref="biblatexml:superscript"/>
      <xs:element minOccurs="0" ref="biblatexml:subscript"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="emphasis" type="xs:string"/>
  <xs:element name="superscript" type="xs:string"/>
  <xs:element name="subscript" type="xs:string"/>
  <xs:element name="nosort" type="xs:string"/>
  <xs:element name="abstract">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="addendum">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="annotation">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="booksubtitle">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="booktitle" type="biblatexml:formattextB"/>
  <xs:element name="booktitleaddon">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="chapter" type="xs:string"/>
  <xs:element name="doi" type="xs:string"/>
  <xs:element name="eid" type="xs:string"/>
  <xs:element name="eprint">
    <xs:complexType mixed="true">
      <xs:attribute name="type" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="eventtitle" type="biblatexml:formattextB"/>
  <xs:element name="file" type="xs:string"/>
  <xs:element name="pdf" type="xs:string"/>
  <xs:element name="id" type="xs:string"/>
  <xs:element name="isan" type="xs:string"/>
  <xs:element name="isbn" type="xs:string"/>
  <!-- validate with pattern ? -->
  <xs:element name="oclc" type="xs:string"/>
  <!-- validate with pattern ? -->
  <xs:element name="lccn" type="xs:string"/>
  <!-- validate with pattern ? -->
  <xs:element name="ismn" type="xs:string"/>
  <xs:element name="isrn" type="xs:string"/>
  <xs:element name="issn" type="xs:string"/>
  <xs:element name="issue">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="issuetitle" type="biblatexml:formattextB"/>
  <xs:element name="issuetitleaddon">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="issuesubtitle">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="iswc" type="xs:string"/>
  <xs:group name="journal">
    <xs:choice>
      <xs:element name="journal">
        <xs:complexType>
          <xs:attribute name="id" use="required"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="journal" type="biblatexml:formattextB"/>
    </xs:choice>
  </xs:group>
  <xs:element name="journaltitle" type="biblatexml:formattextB"/>
  <xs:element name="journalsubtitle">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="label" type="xs:string"/>
  <xs:element name="private">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="biblatexml:library"/>
        <xs:element ref="biblatexml:remarks"/>
        <xs:element ref="biblatexml:topics"/>
        <xs:group ref="biblatexml:user.field"/>
      </xs:choice>
      <xs:attribute name="user" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:group name="user.field">
    <xs:sequence>
      <xs:any namespace="http://localhost/biblatexml/user" processContents="skip"/>
    </xs:sequence>
  </xs:group>
  <xs:element name="library">
    <xs:complexType>
      <xs:group maxOccurs="unbounded" ref="biblatexml:libraryitem"/>
    </xs:complexType>
  </xs:element>
  <xs:group name="libraryitem">
    <xs:sequence>
      <xs:element name="item">
        <xs:complexType>
          <xs:attribute name="location"/>
          <xs:attribute name="ref"/>
          <xs:attribute name="url" type="xs:anyURI"/>
          <xs:attribute name="note"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="mainsubtitle">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="maintitle" type="biblatexml:formattextB"/>
  <xs:element name="maintitleaddon">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="nameaddon" type="xs:string"/>
  <xs:element name="note">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="number" type="xs:string"/>
  <xs:element name="origtitle">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:group ref="biblatexml:formattext"/>
        <xs:element minOccurs="0" ref="biblatexml:romanized"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="romanized" type="xs:string"/>
  <xs:element name="pagetotal" type="xs:string"/>
  <xs:element name="part" type="xs:string"/>
  <xs:element name="reprinttitle">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:group name="series">
    <xs:choice>
      <xs:element name="series">
        <xs:complexType>
          <xs:attribute name="id" use="required"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="series">
        <xs:complexType mixed="true">
          <xs:choice>
            <xs:group ref="biblatexml:formattext"/>
            <xs:group maxOccurs="unbounded" ref="biblatexml:item"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:group>
  <xs:element name="shorthand" type="xs:string"/>
  <xs:element name="shorthandintro" type="xs:string"/>
  <xs:element name="shorttitle" type="biblatexml:formattextB"/>
  <xs:element name="subtitle">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="title" type="biblatexml:formattextB"/>
  <xs:element name="titleaddon">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="url" type="xs:anyURI"/>
  <xs:element name="venue">
    <xs:complexType mixed="true">
      <xs:group ref="biblatexml:formattext"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="version" type="xs:string"/>
  <xs:element name="volume" type="xs:string"/>
  <xs:element name="volumes" type="xs:string"/>
  <xs:element name="usera" type="xs:string"/>
  <xs:element name="userb" type="xs:string"/>
  <xs:element name="userc" type="xs:string"/>
  <xs:element name="userd" type="xs:string"/>
  <xs:element name="usere" type="xs:string"/>
  <xs:element name="userf" type="xs:string"/>
  <xs:group name="entryset">
    <xs:annotation>
      <xs:documentation>special fields</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="entryset">
        <xs:complexType mixed="true">
          <xs:group minOccurs="0" maxOccurs="unbounded" ref="biblatexml:item"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="inset">
    <xs:sequence>
      <xs:element name="entryset" type="xs:string"/>
    </xs:sequence>
  </xs:group>
  <xs:element name="crossref" type="xs:string"/>
  <xs:element name="entrysubtype" type="xs:string"/>
  <xs:element name="execute" type="xs:string"/>
  <xs:element name="hyphenation" type="xs:string"/>
  <xs:element name="indexsorttitle" type="xs:string"/>
  <xs:element name="indextitle" type="xs:string"/>
  <xs:element name="options">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="biblatexml:option"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="option">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="biblatexml:key"/>
        <xs:element ref="biblatexml:value"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="key" type="xs:string"/>
  <xs:element name="value" type="xs:string"/>
  <xs:element name="presort" type="xs:string"/>
  <xs:element name="sortkey" type="xs:string"/>
  <xs:element name="sorttitle" type="xs:string"/>
  <xs:element name="sortyear" type="xs:string"/>
  <xs:element name="useprefix" type="xs:string"/>
  <xs:element name="xref" type="xs:string"/>
  <!-- RANGE FIELDS -->
  <xs:element name="origyear">
    <xs:complexType mixed="true">
      <xs:group minOccurs="0" ref="biblatexml:range"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="year">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:group minOccurs="0" ref="biblatexml:range"/>
        <xs:element minOccurs="0" ref="biblatexml:localyear"/>
      </xs:sequence>
      <xs:attribute name="type">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="approximate"/>
            <xs:enumeration value="assumed"/>
            <xs:enumeration value="converted"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!--
    approximate when exact date is uncertain, 
    assumed when it is known but not stated on the document
    and converted when it is stated in a different calendar
  -->
  <xs:attributeGroup name="attlist.year">
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="approximate"/>
          <xs:enumeration value="assumed"/>
          <xs:enumeration value="converted"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="localyear">
    <xs:complexType mixed="true">
      <xs:group minOccurs="0" ref="biblatexml:range"/>
      <xs:attributeGroup ref="biblatexml:attlist.localyear"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.localyear">
    <xs:attribute name="calendar" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="julian"/>
          <xs:enumeration value="hebrew"/>
          <xs:enumeration value="islamic"/>
          <xs:enumeration value="hijrishamsi"/>
          <xs:enumeration value="revolutionary"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="note"/>
  </xs:attributeGroup>
  <xs:group name="range">
    <xs:sequence>
      <xs:element ref="biblatexml:start"/>
      <xs:element ref="biblatexml:end"/>
    </xs:sequence>
  </xs:group>
  <xs:element name="start" type="xs:string"/>
  <xs:element name="end" type="xs:string"/>
  <xs:element name="pages">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0">
        <xs:group ref="biblatexml:range"/>
        <xs:element ref="biblatexml:list"/>
      </xs:choice>
      <xs:attributeGroup ref="biblatexml:attlist.pages"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="list" type="xs:string"/>
  <xs:attributeGroup name="attlist.pages">
    <xs:attribute name="pagination">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="page"/>
          <xs:enumeration value="column"/>
          <xs:enumeration value="line"/>
          <xs:enumeration value="verse"/>
          <xs:enumeration value="paragraph"/>
          <xs:enumeration value="none"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="bookpagination">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="page"/>
          <xs:enumeration value="column"/>
          <xs:enumeration value="line"/>
          <xs:enumeration value="verse"/>
          <xs:enumeration value="paragraph"/>
          <xs:enumeration value="none"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="part"/>
  </xs:attributeGroup>
  <!-- e.g. when a book has parts in different scripts, with respective paginations -->
  <!-- INTEGER FIELDS -->
  <xs:element name="day" type="xs:integer"/>
  <xs:element name="edition">
    <xs:complexType>
      <xs:simpleContent>
        <xs:restriction base="xs:anyType">
          <xs:simpleType>
            <xs:union memberTypes="xs:integer">
              <xs:simpleType>
                <xs:restriction base="xs:token">
                  <xs:length value="0"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:union>
          </xs:simpleType>
          <xs:attribute name="string"/>
        </xs:restriction>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="month" type="xs:integer"/>
  <xs:element name="urlday" type="xs:integer"/>
  <xs:element name="urlmonth" type="xs:integer"/>
  <xs:element name="urlyear" type="xs:integer"/>
  <!-- DATE FIELDS -->
  <xs:element name="date">
    <xs:simpleType>
      <xs:union memberTypes="xs:date xs:integer"/>
    </xs:simpleType>
  </xs:element>
  <xs:element name="urldate" type="xs:date"/>
  <!-- LITERAL LISTS -->
  <xs:attributeGroup name="attlist.lists">
    <xs:attribute name="andothers" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:group name="institution">
    <xs:choice>
      <xs:element name="institution">
        <xs:complexType>
          <xs:attribute name="id" use="required"/>
          <xs:attribute name="andothers">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="institution">
        <xs:complexType mixed="true">
          <xs:group minOccurs="0" maxOccurs="unbounded" ref="biblatexml:item"/>
          <xs:attribute name="andothers">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:group>
  <xs:group name="school">
    <xs:choice>
      <xs:element name="school">
        <xs:complexType>
          <xs:attribute name="id" use="required"/>
          <xs:attribute name="andothers">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="school">
        <xs:complexType mixed="true">
          <xs:group minOccurs="0" maxOccurs="unbounded" ref="biblatexml:item"/>
          <xs:attribute name="andothers">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:group>
  <xs:group name="organization">
    <xs:choice>
      <xs:element name="organization">
        <xs:complexType>
          <xs:attribute name="id" use="required"/>
          <xs:attribute name="andothers">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="organization">
        <xs:complexType mixed="true">
          <xs:group minOccurs="0" maxOccurs="unbounded" ref="biblatexml:item"/>
          <xs:attribute name="andothers">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:group>
  <xs:group name="location">
    <xs:choice>
      <xs:element name="location">
        <xs:complexType>
          <xs:attribute name="id" use="required"/>
          <xs:attribute name="andothers">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="location">
        <xs:complexType mixed="true">
          <xs:group minOccurs="0" maxOccurs="unbounded" ref="biblatexml:item"/>
          <xs:attribute name="andothers">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:group>
  <xs:group name="origlocation">
    <xs:choice>
      <xs:element name="origlocation">
        <xs:complexType>
          <xs:attribute name="id" use="required"/>
          <xs:attribute name="andothers">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="origlocation">
        <xs:complexType mixed="true">
          <xs:group minOccurs="0" maxOccurs="unbounded" ref="biblatexml:item"/>
          <xs:attribute name="andothers">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:group>
  <xs:group name="publisher">
    <xs:choice>
      <xs:element name="publisher">
        <xs:complexType>
