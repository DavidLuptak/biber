% Completely nonexistent field and field (alias) not valid for this type
% Testing a field valid for all entrytypes (ABSTRACT)
% Testing missing mandatory field
@ETA{c2,
  TITLE       = {Limits of Things},
  DATE        = {2003-10-01},
  BADFIELD    = {Something},
  JOURNAL     = {Some journal},
  ABSTRACT    = {Some text}
}

